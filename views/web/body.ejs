<% include header %>
<div class="container" style="margin-top: 60px;">
    <div class="row">
        <% if(!user){ %>
        <div class="col-md-12">
            <% include itemList %>
        </div>
        <% } else{ %>
        <div class="col-md-12" ng-app="lists">
            <div ng-controller="list">
                <div class="col-md-8">
                    <% include itemLists %>
                </div>
                <div class="col-md-4">
                    <% include welcome %>
                    <% include news %>
                </div>
            </div>
        </div>
        <% } %>
    </div>
</div>
<% include footer %>